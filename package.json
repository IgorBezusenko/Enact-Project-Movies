{
  "name": "enact-form",
  "version": "1.0.0",
  "description": "A general template for an Enact Sandstone application for webOS TVs",

  "author": "Bezusenko Igor",
  "main": "src/index.js",
  "scripts": {
    "serve": "enact serve",
    "ares": "ares-package -o ipk dist",
    "pack": "enact pack",
    "pack-p": "enact pack -p",
    "watch": "enact pack --watch",
    "clean": "enact clean",
    "lint": "enact lint .",
    "license": "enact license",
    "test": "enact test",
    "test-watch": "enact test --watch",
    "predeploy": "npm run pack-p",
    "deploy": "gh-pages -d dist"
  },
  "license": "UNLICENSED",
  "private": true,
  "repository": "",
  "engines": {
    "npm": ">=6.9.0"
  },
  "enact": {
    "theme": "sandstone"
  },
  "eslintConfig": {
    "extends": "enact"
  },
  "eslintIgnore": [
    "node_modules/*",
    "build/*",
    "dist/*"
  ],
  "dependencies": {
    "@enact/core": "^4.0.0",
    "@enact/i18n": "^4.0.0",
    "@enact/moonstone": "^4.0.0",
    "@enact/sandstone": "^2.0.0-alpha.3",
    "@enact/spotlight": "^4.0.0",
    "@enact/ui": "^4.0.0",
    "@enact/webos": "^4.0.0",
    "@hookform/error-message": "^2.0.0",
    "axios": "^0.21.1",
    "ilib": "npm:ilib-webos@^14.6.2-webos1",
    "prop-types": "^15.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-hls-player": "^3.0.1",
    "react-hot-loader": "^4.13.0",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "reactjs-localstorage": "^1.0.1",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "screenfull": "^5.1.0"
  }
